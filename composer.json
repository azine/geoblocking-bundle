{
    "name": "azine/geoblocking-bundle",
    "type": "symfony-bundle",
    "description": "Bundle to filter/geoblock the access of visitors to your pages by location ",
    "keywords": ["geoblocking", "country", "location"],
    "homepage": "https://github.com/azine/geoblocking-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Azine IT Services AG",
            "email": "github@azine-it.ch"
        }
    ],
    "config": {
        "process-timeout": 590,
        "github-protocols": ["https", "git", "ssh"],
        "github-domains": ["github.com"]
    },
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.6 || >=3.0",
        "symfony/symfony": ">=2.6 || >=3.0"
    },
    "require-dev": {
        "twig/extensions": "~1.0"
    },
    "autoload": {
        "psr-0": { "Azine\\GeoBlockingBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Azine/GeoBlockingBundle"
}